{
  "extends": ["html-validate:standard"],
  "rules": {
        "require-sri": "warn",
        "script-type": "warn",
        "wcag/h30": "warn",
        "wcag/h37": "warn",
        "heading-level": "warn",
        "element-permitted-content": "warn",
        "input-missing-label": "warn",
        "no-deprecated-attr": "warn",
        "attribute-allowed-values": "warn",
        "element-name": "warn"
  },
  "elements": [
    "html5"      
  ]
}